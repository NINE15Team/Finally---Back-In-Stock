    "dev-hadi": "npm run shopify app config use finally-bis-hadi && shopify app dev",
    "dev-tunnel": "npm run dev -- --tunnel-url  https://mite-perfect-narwhal.ngrok-free.app:3000",
    "dev-tunnel-reset": "npm run dev -- --tunnel-url   https://mite-perfect-narwhal.ngrok-free.app:3000",
