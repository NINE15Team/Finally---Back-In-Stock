    "deploy:staging": "npm run shopify app config use staging && shopify app deploy",
    "env": "shopify app env",
    "start": "npx prisma generate &&  npm run build && remix-serve ./build/server/index.js",
    "node": "node server.js",
    "docker-start": "npm run setup && npm run start",
